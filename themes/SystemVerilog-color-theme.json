		{
			"scope": [
				"keyword.block.sv"
			],
			"settings": {
				"foreground": "#9ACD32"
			}
		},
		{
			"scope": [
				"keyword.control.sv"
			],
			"settings": {
				"foreground": "#DDA0DD"
			}
		},
		{
			"scope": [
				"keyword.type.sv"
			],
			"settings": {
				"foreground": "#DDA0DD"
			}
